{
  "builds": [
    {
      "src": "pages/api/main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "pages/api/main.go"
    },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "public, max-age=3600, immutable" },
      "dest": "/$1"
    }
  ]
}
