{
  "builds": [
    {
      "src": "pages/api/main.rs", // Updated path
      "use": "@vercel/rust"
    }
  ],
  "routes": [
    {
      "src": "/api/(?<video_id>[^/]+)", // Updated route
      "dest": "/api/main.rs" // Updated destination
    },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "public, max-age=3600, immutable" },
      "dest": "/$1"
    }
  ]
}
