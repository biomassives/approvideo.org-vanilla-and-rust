{
  "builds": [
    {
      "src": "pages/api/main.rs",
      "use": "@vercel/rust@0.8.0" 
    }
  ],
  "routes": [
    {
      "src": "/api/(?<video_id>[^/]+)",  
      "dest": "/api/main.rs"  
    },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "public, max-age=3600, immutable" },
      "dest": "/$1"
    }
  ]
}
